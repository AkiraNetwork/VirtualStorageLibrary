{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "AkiraNetwork/VirtualStorageLibrary.Test/**"  // VirtualStorageLibrary.TestÇèúäO
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "../docs",
    "template": [
      "default",
      "modern",
      "templates/material"
    ],
    "globalMetadata": {
      "_appName": "AkiraNetwork",
      "_appTitle": "AkiraNetwork",
      "_enableSearch": true,
      "pdf": false,
      "_disableContribution": true,
      "favicon": "images/tree.png",
      "_appFaviconPath": "images/tree_32x32.svg",
      "_appLogoPath": "images/tree_51x51.svg",
      "_enableNewTab": "true"
    }
  }
}